{
  "openai": {
    "default_model": "gpt-5",
    "max_tokens": 8000,
    "temperature": 0.1,
    "reasoning_effort": "high",
    "api_key_env_var": "OPENAI_API_KEY"
  },
  "extraction": {
    "timeout_seconds": 120,
    "retry_attempts": 3,
    "use_minimal_prompts": true,
    "max_prompt_tokens": 5000
  },
  "pdf_reduction": {
    "enabled": true,
    "max_file_size_mb": 0.1,
    "strategy": "first_and_last_pages",
    "first_pages": 2,
    "last_pages": 2,
    "min_pages_to_keep": 4
  },
  "validation": {
    "isin_pattern": "^[A-Z]{2}[0-9A-Z]{10}$",
    "max_file_size_mb": 100
  },
  "files": {
    "system_prompt": "minimal_system_prompt.md",
    "full_system_prompt": "system_prompt.md",
    "compact_schema": "compact_schema.json",
    "full_schema": "output_schema.json"
  }
}